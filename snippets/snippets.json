{
   "md-autocomplete":{
        "prefix":"md-autocomplete:full",
        "body":[
          "<md-autocomplete",
          "   ng-disabled=\"${1:ctrl}.isDisabled}\"",
          "   md-no-cache=\"${1:ctrl}.noCache\"",
          "   md-selected-item=\"${1:ctrl}.selectedItem\"",
          "   md-search-text-change=\"${1:ctrl}.searchTextChange(${1:ctrl}.searchText)\"",
          "   md-search-text=\"${1:ctrl}.searchText\"",
          "   md-selected-item-change=\"${1:ctrl}.selectedItemChange(item)\"",
          "   md-items=\"${3:item} in ${1:ctrl}.querySearch(${1:ctrl}.searchText)\"",
          "   md-item-text=\"${3:item}.display\"",
          "   md-min-length=\"0\"",
          "   placeholder=\"${2:placeholder}\">"],
        "description":"angular material : all auto complete options"
    },
    "md-bottom-sheet":{
        "prefix":"md-bottom-sheet",
        "body":[
          "<md-bottom-sheet class=\"md-grid\" layout=\"column\" ng-cloak>",
          "  <div layout=\"row\" layout-align=\"center center\">",
          "    <h4>With clickOutsideToClose option, drag down or press ESC to close</h4>",
          "  </div>",
          "  <div>",
          "    <md-list flex layout=\"row\" layout-align=\"center center\">",
          "      <md-list-item ng-repeat=\"item in items\">",
          "        <md-button class=\"md-grid-item-content\" ng-click=\"listItemClick($index)\">",
          "          <md-icon md-svg-src=\"{{item.icon}}\"></md-icon>",
          "          <div class=\"md-grid-text\"> {{ item.name }} </div>",
          "        </md-button>",
          "      </md-list-item>",
          "    </md-list>",
          "  </div>",
          "</md-bottom-sheet>"
        ],
        "description":"angular material : bottom sheet"
    },
    "":{
        "prefix":"",
        "body":[
            "<md-autocomplete flex required",
            "   md-input-name=\"${1:autocompleteField}\"",
            "   md-input-minlength=\"${2:2}\"",
            "   md-input-maxlength=\"${3:18}\"",
            "   md-no-cache=\"${4:ctrl}.noCache\"",
            "   md-selected-item=\"${4:ctrl}.selectedItem\"",
            "   md-search-text=\"${4:ctrl}.searchText\"",
            "   md-items=\"${5:item} in ${4:ctrl}.querySearch(${4:ctrl}.searchText)\"",
            "   md-item-text=\"${5:item}.display\"",
            "   md-floating-label=\"Favorite state\">",
            "<md-item-template>",
            "<span md-highlight-text=\"${4:ctrl}.searchText\">{{${5:item}.display}}</span>",
            "</md-item-template>",
            "<div ng-messages=\"searchForm.autocompleteField.$error\" ng-if=\"searchForm.autocompleteField.$touched\">",
            "   <div ng-message=\"required\">You <b>must</b> have a favorite state.</div>",
            "   <div ng-message=\"minlength\">Your entry is not long enough.</div>",
            "   <div ng-message=\"maxlength\">Your entry is too long.</div>",
            "</div>",
            "</md-autocomplete>"
        ],
        "description":"angular material :"
    },
    "":{
        "prefix":"",
        "body":[""],
        "description":"angular material :"
    }
}