{
   "md-autocomplete":{
        "prefix":"md-autocomplete:full",
        "body":[
          "<md-autocomplete",
          "   ${ng-disabled=\"${1:ctrl}.isDisabled\"}",
          "   ${md-no-cache=\"${1:ctrl}.noCache\"}",
          "   ${md-selected-item=\"${1:ctrl}.selectedItem\"}",
          "   ${md-search-text-change=\"${1:ctrl}.searchTextChange(${1:ctrl}.${2:searchText})\"}",
          "   ${md-search-text=\"${1:ctrl}.${2:searchText}\"}",
          "   ${md-selected-item-change=\"${1:ctrl}.selectedItemChange(item)\"}",
          "   ${md-items=\"${3:item} in ${1:ctrl}.querySearch(${1:ctrl}.${2:searchText})\"}",
          "   ${md-item-text=\"${3:item}.${4:display}\"}",
          "   ${md-min-length=\"${5:0}\"}",
          "   ${md-floating-label=\"Favorite state\"}",
          "   ${placeholder=\"${6:placeholder}\">}",
          "<md-item-template>",
          "    <span md-highlight-text=\"${1:ctrl}.${2:searchText}\" md-highlight-flags=\"${7:^i}\">{{${3:item}.display}}</span>",
          "</md-item-template>",
          "<md-not-found>",
          "    No states matching \"{{${1:ctrl}.${2:searchText}}}\" were found.",
          "    <a ng-click=\"${1:ctrl}.newState(${1:ctrl}.${2:searchText})\">${8:Create a new one!}</a>",
          "</md-not-found>"
          ],
        "description":"angular material : all auto complete options"
    },
    "md-autocomplete-tpl":{
        "prefix":"md-autocomplete:tpl",
        "body":[
          "<md-autocomplete",
          "   ${ng-disabled=\"${1:ctrl}.isDisabled\"}",
          "   ${md-no-cache=\"${1:ctrl}.noCache\"}",
          "   ${md-selected-item=\"${1:ctrl}.selectedItem\"}",
          "   ${md-search-text-change=\"${1:ctrl}.searchTextChange(${1:ctrl}.${2:searchText})\"}",
          "   ${md-search-text=\"${1:ctrl}.${2:searchText}\"}",
          "   ${md-selected-item-change=\"${1:ctrl}.selectedItemChange(item)\"}",
          "   ${md-items=\"${3:item} in ${1:ctrl}.querySearch(${1:ctrl}.${2:searchText})\"}",
          "   ${md-item-text=\"${3:item}.${4:display}\"}",
          "   ${md-min-length=\"${5:0}\"}",
          "   ${placeholder=\"${6:placeholder}\">}",
          "   ${md-floating-label=\"Favorite state\"}",
          "   md-menu-class=\"autocomplete-custom-template\"",
          "<md-item-template>",
          "<span class=\"item-title\">",
          "  <md-icon md-svg-icon=\"${7:img/icons/octicon-repo.svg}\"></md-icon>",
          "  <span> {{${3:item}.${8:name}} </span>",
          "</span>",
          "<span class=\"item-metadata\">",
          "  <span class=\"item-metastat\">",
          "    <strong>{{${3:item}.${9:item1}}}</strong>",
          "  </span>",
          "  <span class=\"item-metastat\">",
          "    <strong>{{${3:item}.${10:item2}}}</strong>",
          "  </span>",
          "</span>",
          "</md-item-template>",
          "<md-not-found>",
          "    No states matching \"{{${1:ctrl}.${2:searchText}}}\" were found.",
          "    <a ng-click=\"${1:ctrl}.newState(${1:ctrl}.${2:searchText})\">${8:Create a new one!}</a>",
          "</md-not-found>"
          ],
        "description":"angular material : all auto complete options with custom template"
    }
}