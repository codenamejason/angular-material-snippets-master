{
    "md-autocomplete": {
        "prefix": "md-autocomplete:full",
        "body": [
            "<md-autocomplete",
            "   ${md-items=\"${1:item} in ${2:ctrl}.querySearch(${2:ctrl}.${3:searchText})\"}",
            "   ${md-item-text=\"${1:item}.${4:display}\"}",
            "   ${ng-disabled=\"${2:ctrl}.isDisabled\"}",
            "   ${md-no-cache=\"${2:ctrl}.noCache\"}",
            "   ${md-selected-item=\"${2:ctrl}.selectedItem\"}",
            "   ${md-search-text-change=\"${2:ctrl}.searchTextChange(${2:ctrl}.${3:searchText})\"}",
            "   ${md-search-text=\"${2:ctrl}.${3:searchText}\"}",
            "   ${md-selected-item-change=\"${2:ctrl}.selectedItemChange(item)\"}",
            "   ${md-min-length=\"${1}\"}",
            "   ${md-max-length=\"${2}\"}",
            "   ${md-delay=\"${0}\"}",
            "   ${md-select-on-match=\"true\"}",
            "   ${md-input-id=\"${input-ID}\"}",
            "   ${md-input-name=\"${nameAttribute}\"}",
            "   ${md-floating-label=\"${label}\"}",
            "   ${md-menu-class=\"${dropdown-class}\"}",
            "   ${md-autofocus=\"true\"}",
            "   ${md-autoselect=\"true\"}",
            "   ${placeholder=\"${placeholder}\"}",
            "   >",
            "   <md-item-template>",
            "        <span md-highlight-text=\"${2:ctrl}.${3:searchText}\" md-highlight-flags=\"${13:^i}\">{{${1:item}.${4:display}}}</span>",
            "   </md-item-template>",
            "   <md-not-found>",
            "        No states matching \"{{${2:ctrl}.${3:searchText}}}\" were found.",
            "        ${<a ng-click=\"${2:ctrl}.newState(${2:ctrl}.${3:searchText})\">${Create a new one!}</a>}",
            "   </md-not-found>",
            "</md-autocomplete>"
        ],
        "description": "angular material : all auto complete options"
    },
    "md-autocomplete-tpl": {
        "prefix": "md-autocomplete:tpl",
        "body": [
            "<md-autocomplete",
            "   ${md-items=\"${1:item} in ${2:ctrl}.querySearch(${2:ctrl}.${3:searchText})\"}",
            "   ${md-item-text=\"${1:item}.${4:display}\"}",
            "   ${ng-disabled=\"${2:ctrl}.isDisabled\"}",
            "   ${md-no-cache=\"${2:ctrl}.noCache\"}",
            "   ${md-selected-item=\"${2:ctrl}.selectedItem\"}",
            "   ${md-search-text-change=\"${2:ctrl}.searchTextChange(${2:ctrl}.${3:searchText})\"}",
            "   ${md-search-text=\"${2:ctrl}.${3:searchText}\"}",
            "   ${md-selected-item-change=\"${2:ctrl}.selectedItemChange(item)\"}",
            "   ${md-min-length=\"${5:0}\"}",
            "   ${md-max-length=\"${6:0}\"}",
            "   ${md-delay=\"${7:0}\"}",
            "   ${md-select-on-match=\"true\"}",
            "   ${md-input-id=\"${8:input-ID}\"}",
            "   ${md-input-name=\"${9:nameAttribute}\"}",
            "   ${md-floating-label=\"${10:label}\"}",
            "   ${md-autofocus=\"true\"}",
            "   ${md-autoselect=\"true\"}",
            "   ${placeholder=\"${12:placeholder}\"}",
            "   md-menu-class=\"autocomplete-custom-template\"",
            "   >",
            " <md-item-template>",
            " <span class=\"item-title\">",
            "     <md-icon md-svg-icon=\"${13:img/icons/octicon-repo.svg}\"></md-icon>",
            "     <span> {{${1:item}.${14:name}} </span>",
            " </span>",
            " <span class=\"item-metadata\">",
            "     <span class=\"item-metastat\">",
            "      <strong>{{${1:item}.${15:item1}}}</strong>",
            "      </span>",
            "     <span class=\"item-metastat\">",
            "       <strong>{{${1:item}.${16:item2}}}</strong>",
            "      </span>",
            " </span>",
            " </md-item-template>",
            "   <md-not-found>",
            "        No states matching \"{{${2:ctrl}.${3:searchText}}}\" were found.",
            "        ${17:<a ng-click=\"${2:ctrl}.newState(${2:ctrl}.${3:searchText})\">${18:Create a new one!}</a>}",
            "   </md-not-found>",
            "</md-autocomplete>"
        ],
        "description": "angular material : all auto complete options with custom template"
    },
    "md-bottom-sheet": {
      "prefix": "md-bottom-sheet:standard",
      "body": [
          "<md-bottom-sheet ${class=\"${md-grid}\"} ${1:layout=\"${2:column|row}\"} ${ng-cloak}>",
          "  <div ${3:layout=\"${4:column|row}\"} ${layout-align=\"${center|end|space-around|space-between} ${start|center|end|stretch}\"}>",
          "    ${content}",
          "  </div>",
          "</md-bottom-sheet>"
       ],
      "description": "angular material : standard bottom sheet"
    },
    "md-bottom-sheet-list": {
      "prefix": "md-bottom-sheet:list",
      "body": [          
          "<md-bottom-sheet class=\"md-list md-has-header\" ng-cloak>",
          "  <md-subheader>${subheader}</md-subheader>",
          "  <md-list>",
          "    <md-list-item ng-repeat=\"${item} in ${items}\">",
          "       ${list content}",
          "    </md-list-item>",
          "  </md-list>",
          "</md-bottom-sheet>"
       ],
      "description": "angular material : bottom sheet list template"
    },
    "md-bottom-sheet-grid": {
      "prefix": "md-bottom-sheet:grid",
      "body": [          
          "<md-bottom-sheet class=\"md-grid\" layout=\"column\" ng-cloak>",
            "<div layout=\"row\" layout-align=\"${center center}\">",
            "  ${header content}",
            "</div>",
            "<div>",
            "  <md-list flex layout=\"row\" layout-align=\"center center\">",
            "    <md-list-item ng-repeat=\"${item} in ${items}\">",
            "       ${list item content}",
            "    </md-list-item>",
            "  </md-list>",
            "</div>",
          "</md-bottom-sheet>"
       ],
      "description": "angular material : bottom sheet grid template"
    },
    "md-button": {
        "prefix": "md-button:simple",
        "body": [
            "<md-button aria-label=\"${1:button-label}\"> ${1:button-label} </md-button>"
        ],
        "description": "angular material : simple button with no options"
    },
    "md-button-full": {
        "prefix": "md-button:full",
        "body": [
            "<md-button ${href=\"${1:url}\"}",
            "           ${id=\"${2:button-id}\"}",
            "           class=\"${3:button-class|md-primary}\"",
            "           ${ng-disabled=\"true\"}",
            "           ${md-ripple-size=\"${4:full | partial | auto}\"}",
            "           ${md-no-ink=\"true\"}",
            "           aria-label=\"${5:button-label}\"> ${5:button-label} </md-button>"
        ],
        "description": "angular material : button with all options"
    },
    "md-button-icon": {
      "prefix": "md-button:icon",
      "body": [
          "<md-button aria-label=\"${1:button-label}\" ${class=\"${md-mini|md-fab}\"}>",
          "   <md-icon md-svg-src=\"${your/icon}.svg\"></md-icon>",
          "   ${1:button-label}",
          "</md-button>"
       ],
      "description": "angular material : button with icon class options for mini or fab"
    },
     "md-card": {
        "prefix": "md-card:standard",
        "body": [
            "<md-card>",
            "  <img src=\"${card-image.png}\" class=\"md-card-image\" alt=\"${image caption}\">",
            "  <md-card-content>",
            "    <h2>${Card headline}</h2>",
            "    <p>${Card content}</p>",
            "  </md-card-content>",
            "  <md-card-actions layout=\"row\" layout-align=\"${end center}\">",
            "    <md-button>${Action 1}</md-button>",
            "    <md-button>${Action 2}</md-button>",
            "  </md-card-actions>",
            "</md-card>"
        ],
        "description": "angular material : standard card"
    },
     "md-checkbox": {
        "prefix": "md-checkbox:full",
        "body": [
           "<md-checkbox ${ng-model=\"${1:model}\"}",
           "             ${name=\"${2:name}\"}",
           "             ${ng-true-value=\"${3:true-value}\"}",
           "             ${ng-false-value=\"${4:false-value}\"}",
           "             ${ng-change=\"${5:change-value}\"}",
           "             ${md-no-ink=\"true\"}",
           "             aria-label=\"${6:label}\"> ${6:label} <md-checkbox>"
        ],
        "description": "angular material : checkbox full options"
    },
    "md-checkbox-simple": {
      "prefix": "md-checkbox:simple",
      "body": [
          "<md-checkbox name=\"${name}\" aria-label=\"${1:label}\">${1:label}</md-checkbox>"
       ],
      "description": "angular material : simple checkbox"
    },
    "md-chip": {
      "prefix": "md-chip:single",
      "body": [
          "<md-chip>{{$chip}}</md-chip>"
       ],
      "description": "angular material : single md-chip"
    },
    "md-chip-remove": {
      "prefix": "md-chip-remove",
      "body": [
          "<md-chips><button md-chip-remove>${remove-title}</button></md-chips>"
       ],
      "description": "angular material : chip remove"
    },
    "md-layout": {
      "prefix": "md-layout",
      "body": [
          "<md-content layout-${padding|fill|wrap|nowrap|margin} ${layout=\"${2:row | column}\"} ${layout-align=\"${3:start|center|end|space-around|space-between} ${4:start|center|end|stretch}\"}>",
          "  ${5:content}",
          "</md-content>"
       ],
      "description": "angular material : the md-content tag with layout options"
    },
    "md-datepicker": {
      "prefix": "md-datepicker:full",
      "body": [
          "<md-datepicker ${ng-model=\"${1:model}\"}",
          "               ${ng-ng-change=\"${2:expression}\"}",
          "               ${ng-min-date=\"${3:Date}\"}",
          "               ${ng-max-date=\"${4:Date}\"}",
          "               ${ng-date-filter=\"${5:function(Date): boolean}\"}",
          "               ${md-placeholder=\"${6:String}\"}",
          "               ${ng-disabled=\"true\"}",
          "               ${ng-required=\"true\"}></md-datepicker>"
       ],
      "description": "angular material : date picker with full options"
    },
    "md-dialog": {
      "prefix": "md-dialog:standard",
      "body": [
        "<md-dialog aria-label=\"${label}\">",
        "   <md-dialog-content>",
        "      ${content}",
        "   </md-dialog-content>",
        " <md-dialog-actions>",
        "   <md-button ng-click=\"closeDialog()\" class=\"md-primary\">Close Dialog</md-button>",
        " </md-dialog-actions>",
        "</md-dialog>"
       ],
      "description": "angular material : standard dialog"
    },
    "md-divider": {
      "prefix": "md-divider",
      "body": [
          "<md-divider ${md-inset}></md-divider>"
       ],
      "description": "angular material : divider"
    },
    "md-fab-speed-dial": {
      "prefix": "md-fab-speed-dial",
      "body": [
            "<md-fab-speed-dial md-direction=\"${!:up|left|right|down}\" class=\"${2:md-fling|md-scale} ${3:md-fab-top-left|md-fab-top-right|md-fab-bottom-left|md-fab-bottom-right (container element MUST = postion:absolute || relative)}\">",
            "  <md-fab-trigger>",
            "    <md-button aria-label=\"${4:label}\"><md-icon icon=\"${5:svg-icon-url}.svg\"></md-icon></md-button>",
            "  </md-fab-trigger>",
            "  <md-fab-actions>",
            "    <md-button aria-label=\"${6:label}\">",
            "      <md-icon icon=\"${7:svg-icon-url}.svg\"></md-icon>",
            "    </md-button>",
            "    <md-button aria-label=\"${8:svg-icon-url}\">",
            "      <md-icon icon=\"/${7:svg-icon-url}.svg\"></md-icon>",
            "    </md-button>",
            "  </md-fab-actions>",
            "</md-fab-speed-dial>"
       ],
      "description": "angular material : md-fab-speed-dial standard"
    },
    "md-fab-toolbar": {
      "prefix": "md-fab-toolbar",
      "body": [
            "<md-fab-toolbar md-direction=\"${!:up|left|right|down}\" class=\"${2:md-fab-top-left|md-fab-top-right|md-fab-bottom-left|md-fab-bottom-right (container element MUST = postion:absolute || relative)}\">",
            "  <md-fab-trigger>",
            "    <md-button aria-label=\"${4:label}\"><md-icon icon=\"${5:svg-icon-url}.svg\"></md-icon></md-button>",
            "  </md-fab-trigger>",
            "  <md-fab-actions>",
            "    <md-button aria-label=\"${6:label}\">",
            "      <md-icon icon=\"${7:svg-icon-url}.svg\"></md-icon>",
            "    </md-button>",
            "    <md-button aria-label=\"${8:label}\">",
            "      <md-icon icon=\"/${7:svg-icon-url}.svg\"></md-icon>",
            "    </md-button>",
            "  </md-fab-actions>",
            "</md-fab-toolbar>"
       ],
      "description": "angular material : md-fab-toolbar standard"
    },
    "md-single-grid-list-tile": {
      "prefix": "md-grid-list-tile:single",
      "body": [
           "<md-grid-tile md-rowspan=\"${number}\" md-colspan=\"${number}\" md-colspan-sm=\"${number}\">",
           "    ${content}",
           "</md-grid-tile>"
       ],
      "description": "angular material : single grid list tile"
    },
    "md-grid-list-tile-with-footer": {
      "prefix": "md-grid-list-tile:footer",
      "body": [
           "<md-grid-tile md-rowspan=\"${number}\" md-colspan=\"${number}\" md-colspan-sm=\"${number}\">",
           "  <md-grid-tile-footer>",
           "    ${content}",
           "  </md-grid-tile-footer>",
           "</md-grid-tile>"
       ],
      "description": "angular material : grid-list-tile-with-footer"
    },
    "md-grid-list": {
      "prefix": "md-grid-list:standard",
      "body": [
           "<md-grid-list md-cols=\"${1:number}\" md-gutter=\"${2:number}\" md-row-height=\"${3:ratio}:${4:ration}\">",
           "  <md-grid-tile></md-grid-tile>",
           "</md-grid-list>"
       ],
      "description": "angular material : grid list with options"
    },
    "me-menu": {
      "prefix": "md-menu:full",
      "body": [
            "<md-menu ${md-offset=\"${offset:x} ${offset:y}\"} ${md-position-mode=\"target-right target\"}>",
            "  <md-button aria-label=\"${label}\" class=\"${md-icon-button}\" ng-click=\"${1:ctrl}.openMenu($mdOpenMenu, $event)\" ${md-prevent-menu-close=\"md-prevent-menu-close\"}>",
            "    <md-icon md-menu-origin md-svg-icon=\"${svg-icon-url}\"></md-icon>",
            "  </md-button>",
            "  <md-menu-content width=\"${number}\">",
            "    ${content}",
            "  </md-menu-content>",
            "</md-menu>"
       ],
      "description": "angular material : full menu"
    },
    "md-icon": {
      "prefix": "md-icon",
      "body": [
          "<md-icon md-svg-src=\"${1:url-of-icon}\" aria-label=\"${2:label}\" class=\"s${number}\"></md-icon>"
       ],
      "description": "angular material : standard md-icon"
    },
    "md-ink-ripple": {
      "prefix": "md-ink-ripple",
      "body": [
          "md-ink-ripple=\"{{ string (color) | boolean }}\""
       ],
      "description": "angular material : md-ink-ripple options"
    },
    "md-input-container": {
      "prefix": "md-input-container:single",
      "body": [
          "<md-input-container ${md-no-float}>",
          "${content}",
          "</md-input-container>"
       ],
      "description": "angular material : single md container. Wrapper for other input elements"
    },
    "input": {
      "prefix": "md-input:single",
      "body": [
          "<input ${name=\"${1:name}\"}", 
          "  ${type=\"${type}\"}",
          "  ${ng-model=\"${lastname}\"} ",
          "  ${required} ",
          "  ${md-maxlength=\"${number\"}",
          "  ${minlength=\"${number}\"}",
          "  ${placeholder=\"${placeholder}\"}",
          "  aria-label=\"${1:name}\"",
          "  >"
       ],
      "description": "angular material : standard input"
    },
    "ng-messages": {
      "prefix": "ng-messages:wrapper",
      "body": [
          "<div ng-messages=\"${1:formName}.${elementName}.$error\" ng-show=\"${1:formName}.${elementName}.$dirty\">",
          "   ${content}",
          "</div>"
       ],
      "description": "the wrapper for ng messages"
    },
    "ng-message": {
      "prefix": "ng-message:single",
      "body": [
          "<div ng-message=\"${target}\">${error message content}</div>"
       ],
      "description": "single ng-message element"
    },
    "md-list-single": {
      "prefix": "md-list:single",
      "body": [
          "<md-list-item ${ng-repeat=\"${item} in ${items}\"}>",
          "  ${content}",
          "</md-list-item>"
       ],
      "description": "snippet"
    },
    "md-progress-circle": {
      "prefix": "md-progress-circle",
      "body": [
          "<md-progress-circular md-mode=\"${determinate|indeterminate}\" value=\"${value}\" ${md-diameter=\"${number}\"} ${class:\"${class}\"} aria-label=\"${label}\"></md-progress-circular>"
       ],
      "description": "angular material : circular progress loader"
    },
    "md-progress-linear": {
      "prefix": "md-progress-linear",
      "body": [
          "<md-progress-linear md-mode=\"${determinate|indeterminate}\" value=\"${number}\" md-buffer-value=\"${number}\" class=\"${class}\" aria-label=\"${label}\"></md-progress-linear>"
       ],
      "description": "angular material : md-progress-linear"
    },
    "md-radio-button": {
      "prefix": "md-radio-button:standard",
      "body": [
          "<md-radio-button value=\"${number}\" aria-label=\"${label}\">",
          "   ${label}",
          "</md-radio-button>"
       ],
      "description": "angular material : standard radio button"
    },
    "md-radio-button-full": {
      "prefix": "md-radio-button:full",
      "body": [
          "<md-radio-button value=\"${number}\" ",
          "                 ${name=\"${name}\"}",
          "                 ${value=\"${value}\"}",
          "                 ${ngChange=\"${string}\"}",
          "                 ${ngValue=\"${value}\"}",
          "                 ${ngModel=\"${model}\"}",
          "                 ${ng-repeat=\"${1:item} in ${items}\"}",
          "                 ${md-no-ink=\"true\"}",
          "                 aria-label=\"${label}\"",
          ">",
          "   ${1:item}",
          "</md-radio-button>"
       ],
      "description": "angular material : radio button with full options"
    },
    "md-radio-group": {
      "prefix": "md-radio-group:standard",
      "body": [
          "<md-radio-group ${ng-model=\"${model}\"}>",
          "${content}",
          "</md-radio-group>"
       ],
      "description": "angular material : radio button group standard"
    },
    "md-select": {
      "prefix": "md-select:standard",
      "body": [
            "<md-select ${ng-model=\"${model}\"} ",
            "           ${multiple=\"true\"}",
            "           ${md-on-open=\"${expression-open}\"}",
            "           ${md-on-close=\"${expression-closed}\"}",
            "           ${md-container-class=\"${className}\"}",
            "           ${placeholder=\"${placeholder}\"}",  
            "           ${aria-label=\"${label}\"",
                        ">",
            "  <md-option ng-value=\"${1:option}\" ng-repeat=\"${1:option} in ${options}\">",
            "    {{ ${1:option} }}",
            " </md-option>",
            "</md-select>" 
       ],
      "description": "angular material : select item with options"
    },
    "md-list": {
      "prefix": "md-list:standard",
      "body": [
            "<md-list>",
            "  <md-list-item ${ng-repeat=\"${item} in ${items}\"}>",
            "    ${content}",
            "  </md-list-item>",
            "</md-list>"
       ],
      "description": "angular material : standard list"
    },
    "md-slider": {
      "prefix": "md-slider:standard",
      "body": [
          "<md-slider ${md-discrete} ",
                      "${ng-model=\"${myDiscreteValue}\"}",
                      "${step=\"${number}\"}",
                      "${min=\"${number}\"}",
                      "${max=\"${number}\"}",
          "</md-slider>"
       ],
      "description": "angular material : standard slider"
    },
    "md-switch": {
      "prefix": "md-switch:full",
      "body": [
            "<md-switch ${md-no-ink}",
            "           name=${name}", 
            "           ${ng-true-value=\"${expression}\"}",
            "           ${ng-false-value=\"${expression}\"}",
            "           ${ng-change=\"${string}\"}",
            "           ${ng-disabled=\"${expression}\"}",
            "           ${ng-model=\"${model}\"}",
            "           aria-label=\"{label}\"",
                        ">",
            "  ${content}",
            "</md-switch>"
       ],
      "description": "angular material : switch full options"
    },
    "md-subheader": {
      "prefix": "md-subheader",
      "body": [
          "<md-subheader>${content}</md-subheader>"
       ],
      "description": "angular material : subheader"
    },
    "md-swipe": {
      "prefix": "md-swipe",
      "body": [
          "md-swipe-${2:top|right|bottom|left}=\"${onSwipe()}\""
       ],
      "description": "angular material :  swipe options"
    },
    "md-tooltip": {
      "prefix": "md-tooltip:full",
      "body": [
            "<md-tooltip ${md-visible=\"${expression}\"} ${md-delay=\"${number}\"} ${md-autohide=\"${boolean}\"} ${md-direction=${string}}>",
            "  ${content}",
            "</md-tooltip>"    
       ],
      "description": "angular material : tool tip full options"
    },
    "md-virtual-repeat": {
      "prefix": "md-virtual-repeat",
      "body": [
            "<md-virtual-repeat-container ${md-top-index=\"${number}\"} ${md-auto-shrink=\"${true}\"} ${md-auto-shrink=-min\"${number}\"} ${md-orient-horizontal=\"${boolean}\"} aria-label=\"${label}\">",
            "  <div md-virtual-repeat=\"${1:item} in ${items}\" md-item-size=\"${number}\">",
            "     ${1:item}",
            "  </div>",
            "</md-virtual-repeat-container>"
       ],
      "description": "snippet"
    },
    "typography": {
      "prefix": "md-typography",
      "body": [
          "md-${display-${1-4}|headline|title|subhead|body-${1-2}|caption}"
       ],
      "description": "angular material : typography options"
    },
    "md-whiteframe": {
      "prefix": "md-whiteframe",
      "body": [
          "md-whiteframe=\"${number (between 1-24)}\""
       ],
      "description": "angular material : md-whiteframe options"
    }
}